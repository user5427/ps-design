{
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "db:migrate": "tsx node_modules/typeorm/cli.js migration:run -d src/database/data-source.cli.ts",
        "db:migrate:generate": "tsx node_modules/typeorm/cli.js migration:generate -d src/database/data-source.cli.ts",
        "db:migrate:revert": "tsx node_modules/typeorm/cli.js migration:revert -d src/database/data-source.cli.ts",
        "db:seed": "tsx src/scripts/seed.ts"
    },
