{
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:swagger\"",
    "dev:server": "tsx watch src/server.ts",
    "dev:swagger": "nodemon --watch src/routes --ext ts --exec \"npm run swagger\"",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:deploy": "prisma migrate deploy --config=prisma.config.ts",
    "db:generate": "prisma generate --config=prisma.config.ts",
    "seed": "tsx src/scripts/seed.ts",
    "swagger": "tsx src/scripts/swagger.ts"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/env": "^5.0.3",
    "@fastify/jwt": "^10.0.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "close-with-grace": "^2.3.0",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.1.0",
    "fastify-type-provider-zod": "^6.1.0",
    "pg": "^8.13.1",
    "zod": "^4.1.12",
    "glob": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "swagger-autogen": "^2.23.7",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "prisma": "^7.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "dotenv": "^17.2.3",
    "nodemon": "^3.1.11",
    "@types/glob": "^8.1.0",
    "ts-node": "^10.9.2"
  }
}